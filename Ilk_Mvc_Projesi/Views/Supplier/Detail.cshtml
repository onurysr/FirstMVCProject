@model SupplierViewModel

<h1>@Model.CompanyName</h1>

<div class="row">
    <div class="col-2 text-black-50">Şirket Adı</div>
    <div class="col-2">@Model.CompanyName</div>
</div>

<div class="row">
    <div class="col-2 text-black-50">Temsilci Adı</div>
    <div class="col-2">@Model.ContactName</div>
</div>

<div class="row">
    <div class="col-2 text-black-50">Temsilci Unvanı</div>
    <div class="col-2">@Model.ContactTitle</div>
</div>

<div class="row">
    <div class="col-2 text-black-50">Ülke/Şehir</div>
    <div class="col-2">@Model.Country/@Model.City</div>
</div>
